<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingLeft="@dimen/feed_item_padding_left_right"
    android:paddingRight="@dimen/feed_item_padding_left_right"
    android:background="@null" >

    <ImageView
        android:id="@+id/status_icon_img"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_centerVertical="true"
        android:padding="4dp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/btn_next"
        style="@style/text.14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:inputType="number"
        android:textColor="@color/fbbrown"
        android:textStyle="bold"
        android:visibility="gone" />

    <TextView


        android:layout_marginTop="3dp"
        android:id="@+id/storename"
        style="@style/text.15.prompt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="3dp"
        android:singleLine="false"
        android:text="TITles"
        android:textColor="@color/Red"
       />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="15dp"
        android:src="@drawable/favouritegrey"
        android:visibility="gone"
        android:layout_marginLeft="@dimen/margin_10"
        android:layout_alignBaseline="@+id/storename"
        android:layout_toRightOf="@+id/storename"
        android:id="@+id/imageView6" />
    <ImageView
        android:layout_width="20dp"

        android:layout_height="20dp"
        android:src="@drawable/phonegrey"
        android:layout_marginRight="10dp"

        android:layout_below="@+id/storename"
        android:layout_toRightOf="@+id/btn_next"

        android:id="@+id/imageView5" />
    <TextView
        android:id="@+id/storeno"
        style="@style/text.15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/storename"
        android:layout_toRightOf="@+id/imageView5"
        android:text="storeno"
        android:textColor="@color/dark_gray"
        android:textStyle="bold"/>



    <LinearLayout
        android:id="@+id/newlinear"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/storeno"
        android:layout_toRightOf="@+id/btn_next"
        android:orientation="horizontal"
        android:paddingTop="10dp"
        android:layout_marginBottom="3dp">

        <ImageView

            android:layout_width="20dp"
            android:layout_height="20dp"
            android:src="@drawable/locationgrey"
            android:layout_marginRight="10dp"
            android:layout_below="@+id/storeno"
            android:layout_toRightOf="@+id/btn_next"
            android:id="@+id/imageView3" />

        <TextView
            android:id="@+id/storelocation"
            style="@style/text.15.prompt"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_marginRight="10dp"
            android:singleLine="false"
            android:layout_below="@+id/storename"
            android:layout_toRightOf="@+id/imageView5"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:paddingLeft="4dp"
            android:text="Store location"
            android:textColor="@color/dark_gray" />


    </LinearLayout>

    <ImageView
        android:id="@+id/totalDollar"
        style="@style/text.13"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_above="@+id/newlinear"
        android:layout_alignBaseline="@+id/storename"
        android:layout_alignParentRight="true"


        android:src="@drawable/car"
        />




    <TextView
        android:id="@+id/storedistance"
        style="@style/text.15.prompt"

        android:layout_width="wrap_content"
        android:singleLine="false"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/newlinear"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/totalDollar"
        android:layout_marginLeft="5dp"
        android:focusable="false"
        android:paddingLeft="4dp"
        android:text="distance"
        android:textColor="@color/fbheadercolor" />



</RelativeLayout>